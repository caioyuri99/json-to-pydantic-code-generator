{
  "name": "json-to-pydantic-code-generator",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "prepare": "npm run build"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caioyuri99/json-to-pydantic-code-generator.git"
  },
  "keywords": [
    "json",
    "pydantic",
    "code-generator",
    "python"
  ],
  "author": "Caio Yuri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caioyuri99/json-to-pydantic-code-generator/issues"
  },
  "homepage": "https://github.com/caioyuri99/json-to-pydantic-code-generator#readme",
  "description": "Generate Pydantic models from JSON. A TypeScript tool that analyzes JSON structures and outputs fully typed Pydantic model code.",
  "dependencies": {
    "@jest/globals": "^30.0.2",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/pluralize": "^0.0.33",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
